pipeline {
    agent any
    environment {
        TAG = "${DATE}.${BUILD_NUMBER}"
    }
    stages {
        stage('[MAVEN] Build') {
            steps {
                withMaven() {
                    sh 'mvn clean package'
                }
            }
        }
        
        stage('[DOCKER] Build') {
            steps {
                script {
                    docker.build("ilussencio/crud_cliente:${TAG}")
                }
            }
        }
        
    }
}
